{
  "$schema": "../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "main-init": {
      "description": "主项目初始化",
      "factory": "./main-init/index",
      "schema": "./main-init/schema.json"
    },
    "sub": {
      "description": "子项目生成",
      "factory": "./sub/index",
      "schema": "./sub/schema.json"
    },
    "remote-sub-init": {
      "description": "远程子项目初始化",
      "factory": "./remote-sub-init/index",
      "schema": "./remote-sub-init/schema.json"
    }
  }
}
